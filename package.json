{
  "name": "time-to-go",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "clean": "pnpx rimraf ios android dist",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview:browser": "vite preview",
    "prepare:cap:ios": "ionic capacitor add ios",
    "prepare:cap:android": "ionic capacitor add android",
    "prepare:cap": "capacitor sync",
    "postinstall": "pnpm build && pnpm prepare:cap:android ; pnpm prepare:cap:ios ; pnpm prepare:cap",
    "preview:ios": "ionic cap open ios --verbose",
    "preview:android": "ionic cap open android --verbose",
    "test:e2e": "npm-run-all -p -r dev test:cypress",
    "test:cypress": "cypress run",
    "test:unit": "vitest run",
    "test": "npm-run-all test:unit test:e2e",
    "lint": "eslint ."
  },
  "dependencies": {
    "@capacitor/android": "^7.1.0",
    "@capacitor/app": "7.0.0",
    "@capacitor/core": "7.1.0",
    "@capacitor/haptics": "7.0.0",
    "@capacitor/ios": "7.1.0",
    "@capacitor/keyboard": "7.0.0",
    "@capacitor/status-bar": "7.0.0",
    "@ionic/vue": "^8.5.0",
    "@ionic/vue-router": "^8.5.0",
    "ionicons": "^7.4.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@capacitor/cli": "7.1.0",
    "@ionic/cli": "^7.2.1",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@vitejs/plugin-legacy": "^5.4.3",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.6",
    "cypress": "^13.17.0",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.33.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "terser": "^5.39.0",
    "typescript": "~5.6.3",
    "vite": "^6.2.2",
    "vitest": "^0.34.6",
    "vue-tsc": "^2.2.8"
  },
  "sideEffects": [
    "*.vue"
  ],
  "description": "An Ionic project",
  "packageManager": "pnpm@10.6.3",
  "pnpm": {
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0",
      "vite@>=5.0.0 <=5.4.11": ">=5.4.12"
    },
    "onlyBuiltDependencies": [
      "core-js",
      "cypress",
      "esbuild"
    ]
  }
}
